---
const {
  title = "References",
} = Astro.props;
---

<div class="not-prose mt-10 rounded-2xl border border-slate-200 bg-slate-50 p-6">
  <div class="text-xs font-semibold uppercase tracking-wide text-slate-700">
    {title}
  </div>

  <div class="mt-3 space-y-2 text-sm leading-6 text-slate-600">
    <slot />
  </div>
</div>
